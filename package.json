{
  "name": "room187",
  "version": "1.0.0",
  "description": "An RnB and Hip-hop game show website",
  "main": "app.js",
  "scripts": {
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\""
  },
  "keywords": [
    "music",
    "rnb",
    "hip-hop",
    "event"
  ],
  "author": "Lloyd Bennett",
  "devDependencies": {
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "gsap": "^3.11.5",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^3.1.4",
    "postcss-loader": "^7.0.2",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "text-split": "^0.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^6.7.3",
    "@prismicio/helpers": "^2.3.9",
    "browserify-fs": "^1.0.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "locomotive-scroll": "^4.1.4",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "url": "^0.11.4",
    "url-browserify": "^1.3.0",
    "util": "^0.12.5"
  }
}
