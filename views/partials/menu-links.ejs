<% let menu = navigation.data.menu_links
%>

<ul class="<%- container_class %>">
  <% menu.forEach((l, i) => { %>
    <% let uid = l.link.uid.replace(/[_-]/g, " ");
       let name = uid.replace(/\b\w/g, a => a.toUpperCase());
    %>
  
    <li class="<%- list_class %>">
      <a href="<%- l.link.url %>" class="<%- list_text_class %> relative" <% if(dataAttr) {%><%- dataAttr %><%}%> data-page-trigger>
        <span class="block" data-nav-link-text>
          <%- name %>
        </span>
      </a>
      <% if(mainMenu) {%>
      <div class="nav-menu__hover" data-nav-hover></div>
      <% } %>
    </li>
  <% }) %>
  <li class="<%- list_class %>">
    <a href="<%- siteDetails.data.main_cta_link.url %>" target="_blank" class="<%- list_text_class %> relative" <% if(dataAttr) {%><%- dataAttr %><%}%>>
      <span class="block" data-nav-link-text>
        Tickets
      </span>
    </a>
    <% if(mainMenu) {%>
    <div class="nav-menu__hover" data-nav-hover></div>
    <% } %>
  </li>
</ul>