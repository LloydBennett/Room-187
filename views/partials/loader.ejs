<div class="loader bg--black fixed" data-loader>
  <% preloader.data.gallery.forEach((el) => { %>
    <div class="loader__item thumbnail__inline" style="max-width: <%- el.image.dimensions.width / 1.2 %>px" data-loader-image>
      <img src="<%- el.image.url %>">
      <% console.log(el.image) %>
    </div>
  <% }) %>
  <% if (zoomIn) { %>
    <div class="loader__item loader--page" data-loader-image data-loader-hero>
      <div class="hero__bg overlay" data-bg style="background-image: url('<%- document.data.hero_image.url %>')"></div>
      <div class="hero__g-overlay overlay"></div>
    </div>
  <% } %>
</div>