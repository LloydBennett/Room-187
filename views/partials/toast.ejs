<div class="toast fixed success flex flex--space-btwn z-index--4" data-toast>
  <div class="flex toast__content">
    <div class="toast__icon" data-toast-icon>
      <div class="toast-icon__error">
        <%- include('../partials/svg/icons/24/warning', { cssClass: 'icon--white' }) -%>
      </div>
      <div class="toast-icon__success">
        <%- include('../partials/svg/icons/24/tick', { cssClass: 'icon--white' }) -%>
      </div>
    </div>
    <p class="type--xs type--white" data-toast-message>Yoo</p>
    <div class="toast-icon__loading">
      <div class="loader__icon"></div>
    </div>
  </div>
  <div class="toast__close thumbnail flex--no-shrink" data-toast-close>
    <%- include('../partials/svg/icons/24/close', { cssClass: 'icon--white'}) -%>
  </div>
</div>