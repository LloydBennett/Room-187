<% let page = pageType %>

<%- include('./partials/loader') -%>

<%- include('./partials/head', { page: page } ) -%>

<body>
  <main id="main" data-page="<%- page %>" data-scroll-speed="-14" class="main <%- page %> relative">
    <%- include('./partials/navigation-menu') -%>
    <div>
      <%- include('./partials/navigation-bar') -%>
      <div <% if (page !== "home") { %>class="section--m-p-t"<% } %>>
        <% switch(page) {
          case 'contact' : %>
            <%- include('./pages/contact') %>
            <%  break; %>
          <% case 'about' : %>
            <%- include('./pages/about') -%>
            <% break; %>
          <% default : %>
          <%- include('./pages/home') -%>
        <% } %>
      </div>
      
    </div>
    <% if (page !== "home") { %>
    <%- include('./partials/footer') -%>
    <% } %>
    
  </main>
  
  <script src="/main.js"></script>
</body>


