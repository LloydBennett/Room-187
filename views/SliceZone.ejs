<% getSlices = ((SliceZone) => {
  return SliceZone.reduce((prevSlices, slice) => {
    switch(slice.slice_type) {
      case 'introduction':
        return prevSlices + include('./slices/Introduction.ejs', { slice: slice })
        break;
      case 'testimonials':
        return prevSlices + include('./slices/Testimonials.ejs', { slice: slice })
        break;
      case 'artists':
        return prevSlices + include('./slices/Artists.ejs', { slice: slice })
        break;
      case 'room_key':
        return prevSlices + include('./slices/RoomKey.ejs', { slice: slice })
        break;
      case 'image_gallery_item':
        return prevSlices + include('./slices/ImageGalleryItem.ejs', { slice: slice })
        break;
      case 'video_gallery_item':
        return prevSlices + include('./slices/VideoGalleryItem.ejs', { slice: slice })
        break;
      case 'image_carousel':
        return prevSlices + include('./slices/ImageCarousel.ejs', { slice: slice })
        break;
      default :
        return '<h1>No Slices found</h1>'
    }
    
  }, '') 
}) %> 

<%- getSlices(SliceZone) %>